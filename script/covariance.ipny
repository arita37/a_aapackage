   "source": [
    "# make some data which has a clear covariate shift\n",
    "n_samples_Z = 1000\n",
    "n_samples_X = 100\n",
    "x = 11*np.random.random(n_samples_Z)- 6.0\n",
    "y = x**2 + 10*np.random.random(n_samples_Z) - 5\n",
    "Z = np.c_[x, y]\n",
    "    \n",
    "x = 2*np.random.random(n_samples_X) - 6.0\n",
    "y = x**2 + 10*np.random.random(n_samples_X) - 5\n",
    "X = np.c_[x, y]"
   ]
